    "compilerOptions": {
        "declaration": false,
        "outDir": "../dist/",
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "importHelpers": false,
        "downlevelIteration": true,
        "noImplicitAny": false,
        "noUnusedLocals": false,
        "lib": ["es5"],
        "types": ["node"]
    }
}
